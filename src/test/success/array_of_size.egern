func concat(a: [int], b: [int]): [int] {
    var aLen = len(a)
    var c = int[aLen + len(b)]
    move(a, c, 0)
    move(b, c, aLen)
    return c
}

func move(src: [int], dest: [int], offset: int) {
    var i = 0
    while (i < len(src)) {
        dest[i + offset] = src[i]
        i += 1
    }
}

var a = [1, 2, 3]
var b = [4, 5, 6]
var c = concat(a, b)
