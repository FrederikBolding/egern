class Dog() {
    var legs = 2
    var friends = 3
    func bark() {
        print("woof")
    }
}


class Labrador(): Dog() {
    override var legs = 4
    var age = 3
    override func bark() {
        print("broel") // note Windows likely won't work with non-ascii encodings
    }

    func getLegs(): int {
        return legs
    }
}

var buller = Dog()
var ibrahim = Labrador()
print(buller.legs)
buller.bark()
print(ibrahim.legs)
ibrahim.bark()

var jens = ibrahim as Dog
print(jens.legs)
print(jens.friends)
jens.bark()

var brutus = Dog()
brutus = ibrahim as Dog
print(brutus.legs)
print(brutus.friends)
brutus.bark()
