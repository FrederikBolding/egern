class Dog() {
    var legs = 2
    func bark() {
        print(42)
    }
}


class Labrador(): Dog() {
    override var legs = 4
    override func bark() {
        print(53)
    }
}

var buller = Dog()
var ibrahim = Labrador()
print(buller.legs)
buller.bark()
print(ibrahim.legs)
ibrahim.bark()

var jens = ibrahim as Dog
print(jens.legs)
jens.bark()

var brutus = Dog()
brutus = ibrahim as Dog
print(brutus.legs)
brutus.bark()
